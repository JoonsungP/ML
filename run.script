#!/bin/bash -l

#SBATCH -J downscale
#SBATCH --gres=gpu:1
#SBATCH --nodes=1
#SBATCH --oversubscribe
#SBATCH --nodelist=loki[01]
#SBATCH --ntasks-per-node=1
#SBATCH --cpus-per-task=32
#SBATCH --time=24:00:00
#SBATCH -o %x.o%j
#SBATCH -e %x.e%j

export OMP_NUM_THREADS=1
which conda
conda activate ML
export CUDA_VISIBLE_DEVICE=2
python train.py

exit 0
